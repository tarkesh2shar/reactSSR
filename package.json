{
	"name": "react-ssr-webpack-proxy-server",
	"version": "1.0.0",
	"description": "A webpack based react server side rendering server",
	"main": "index.js",
	"scripts": {
		"clean:client": "rimraf public",
		"clean:server": "rimraf build/images",
		"dev:server": "browser-refresh build/bundle.js",
		"dev:buildServer": "webpack --config webpack.server.js --watch --mode=development",
		"dev:buildClient": "npm run clean:client && webpack --config webpack.client.js --watch --mode=development",
		"dev": "npm-run-all --parallel dev:*",
		"prod:server": "webpack --config webpack.server.js",
		"prod:client": "webpack --config webpack.client.js",
		"prod": "npm run prod:server && npm run clean:server && npm run clean:client && npm run prod:client  && node build/bundle.js",
		"start": "node build/bundle.js"
	},
	"keywords": [
		"reactWebpackSSR"
	],
	"author": "tarkesh2shar",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.19.2",
		"browser-refresh": "^1.7.3",
		"browser-refresh-client": "^1.1.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-http-proxy": "^1.6.0",
		"nodemon": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"pngquant-bin": "^3.1.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.0.0",
		"react-redux": "^7.2.0",
		"react-router-config": "^5.1.1",
		"react-router-dom": "^5.1.2",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"serialize-javascript": "^3.0.0",
		"webpack-node-externals": "^1.7.2"
	},
	"devDependencies": {
		"autoprefixer": "^9.7.5",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"copy-webpack-plugin": "^5.1.1",
		"css-hot-loader": "^1.4.4",
		"css-loader": "^3.2.0",
		"file-loader": "^4.3.0",
		"html-webpack-plugin": "^3.2.0",
		"ignore-loader": "^0.1.2",
		"image-webpack-loader": "^5.0.0",
		"imagemin-pngquant": "^5.0.1",
		"lasso": "^3.3.1",
		"marko": "^4.22.5",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.14.1",
		"postcss-loader": "^3.0.0",
		"rimraf": "^2.6.3",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.0.0",
		"url-loader": "^2.1.0",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.6",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.1"
	}
}
